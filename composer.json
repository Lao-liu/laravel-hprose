{
    "name": "lao-liu/laravel-hprose",
    "description": "Hprose client/server providers for Laravel 5.x or Lumen 5.x",
    "keywords": ["Hprose", "laravel", "lumen", "Rpc", "webservice", "JsonRpc", "XmlRpc"],
    "version":"0.9.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Laoliu",
            "email": "Laoliu@lanmv.com"
        }
    ],
    "type": "library",
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": ">=5.0",
        "hprose/hprose": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "laravel/laravel": ">=5.0"
    },
    "autoload": {
        "psr-4": {
            "Laoliu\\LaravelHprose\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
